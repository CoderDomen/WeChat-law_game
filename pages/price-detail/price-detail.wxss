.price-container {
	width: 100%;
	height: 100%;
	position: relative;
}
.price {
	position: fixed;
	top: 0;
	right: 0;
	left: 0;
	bottom: 0;
	overflow-y: auto;
}
.price-content {
	display: flex;
	flex-direction: column;
	align-items: center;
}
.price .price-logo {
	width: 45.99vh;
	height: 45.44vh;
	margin-top: -156rpx;
}

.price .price-logo.not {
	margin-top: 0 !important;
}

.price .content {
	width: 573rpx;
	font-size: 36rpx;
	color: #fff;
	font-family: Source Han Sans CN;
	font-weight: 400;
	margin-top: -100rpx;
}

.price .content.not {
	width: 45.99vh !important;
	box-sizing: border-box;
	padding: 0 5vh;
	margin-top: 0 !important;
}

.price .content .price-name {
	color: #f9eb0d;
}
.price-photo-wrapper {
	width: 17.36vh;
	height: 17.36vh;
	box-sizing: border-box;
	padding: 10rpx;
	margin-top: 5.48vh;
}
.price-photo-wrapper .price-photo {
	height: 100%;
	width: 100%;
}
.note{
	color: #f9eb0d;
}

.price .addressBtn {
	width: 19.58vh;
	height: 6.15vh;
	margin-top: 11.02vh;
	padding-bottom: 100rpx;
}

.addressImg {
	height: 100%;
	width: 100%;
}
