.navBar {
	position: relative;
	z-index: 999;
	display: flex;
	align-items: center;
}
.navBar .avatar {
	width: 100%;
	display: flex;
	align-items: center;
}
.avatar-warpper {
	width: 5.66vh !important;
	height: 5.66vh !important;
	background-color: #fff;
	border-radius: 50%;
	box-sizing: border-box;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 5rpx;
	overflow: hidden;
	box-shadow: 0px 5px 0px 0px rgba(0, 0, 0, 0.35);
	margin-right: 3.14vh !important;
	margin-left: 0 !important;
}

.avatarBtn {
	height: 100%;
	width: 100%;
	opacity: 0;
	display: block;
	padding: 0 !important;
	margin: 0 !important;
	border: 0 !important;
	position: absolute;
	left: 0;
	top: 0;
}

.avatar-warpper .avatar-content {
	width: 100%;
	height: 100%;
	border-radius: 50%;
	overflow: hidden;
}

.rowCenter {
	display: flex;
	align-items: center;
}
.gold,
.shield {
	/* display: flex; */
	/* align-items: center; */
	width: 9.23vh;
	height: 3.38vh;
	/* width: 150rpx; */
	/* height: 55rpx; */
	position: relative;
}
.gold text,
.shield text {
	height: 100%;
	width: 100%;
	line-height: 55rpx;
	background: rgba(0, 0, 0, 0.17);
	border: 2px solid #1f2f69;
	border-radius: 11px;
	color: #fff;
	font-size: 30rpx;
	font-family: Arial;
	font-weight: 400;
	text-align: center;
}

.icon {
	position: absolute;
	/* left: -36rpx; */
	left: -2.21vh;
}
.gold {
	margin-right: 57rpx;
}
.gold image {
	width: 4.43vh;
	height: 4.06vh;
	/* width: 72rpx; */
	/* height: 66rpx; */
}

.shield image {
	width: 4.43vh;
	height: 4.74vh;
	/* width: 72rpx; */
	/* height: 77rpx; */
}

.back {
	width: 100%;
	height: 100%;
	display: flex;
	color: #fff;
	align-items: center;
	font-family: PingFang SC;
}

.backNarrow {
	width: 50rpx;
	height: 50rpx;
	margin-right: 10rpx;
}

.titleCenter {
	position: absolute;
left: 50%;
transform: translate3d(-50%, 0, 0);
font-weight: 800;
color: #fff;
}