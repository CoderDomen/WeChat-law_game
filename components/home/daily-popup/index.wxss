.daily-container {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	box-sizing: border-box;
	padding: 0 25rpx;
	z-index: 1000;
}

.popup {
	width: 701rpx;
	height: 783rpx;
	position: relative;
	top: 50%;
	transform: translate3d(0, -50%, 0);
}
.popup .bg {
	height: 100%;
	width: 100%;
	position: absolute;
}
.popup .closeBtn {
	width: 86rpx;
	height: 82rpx;
	position: absolute;
	top: 0;
	right: -22rpx;
	z-index: 999;
}

.popup .daily-content {
	width: 100%;
	height: 100%;
	position: relative;
}

.popup .daily-content .content {
	display: flex;
	flex-direction: column;
	padding-top: 160rpx;
	padding: 160rpx 50rpx 0;
}

.date {
	font-size: 28rpx;
	font-weight: 500;
	color: rgba(103, 85, 53, 1);
}

.date .text {
	width: 14.28%;
	height: 2.47vh;
	text-align: center;
	display: inline-block;
}

.head {
	width: 100%;
	display: flex;
	justify-content: space-between;
	padding-bottom: 1.84vh;
	border-bottom: 1px solid rgb(241, 211, 189);
}
.body {
	width: 100%;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.body .text {
	/* margin-top: 1.84vh; */
	margin-top: 20rpx;
}

.body .hook {
	width: 42rpx;
	height: 42rpx;
}

.prize {
	width: 598rpx;
	height: 120rpx;
	margin: 0 auto;
	background: rgba(252, 218, 169, 1);
	border-radius: 60px;
	display: flex;
	align-items: center;
	justify-content: space-around;
	position: absolute;
	bottom: 3.07vh;
}

.prize .left {
	display: flex;
	align-items: center;
}

.prize .left text {
	font-size: 33rpx;
	font-family: Microsoft YaHei;
	font-weight: 400;
	color: rgba(94, 56, 45, 1);
}

.prize .photo {
	width: 134rpx;
	height: 133rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
}

.receive {
	width: 228rpx;
	height: 97rpx;
}

.prize .photo image {
	width: 72rpx;
	height: 78rpx;
	margin: 27rpx 0 0 33rpx;
}
